<?xml version="1.0" ?>
<robot name="robot" xmlns:xacro="http://www.ros.org/wiki/xacro">

<xacro:include filename="$(find diffy)/models/urdf/materials.xacro" />
<xacro:include filename="$(find diffy)/models/urdf/robot.trans" />
<xacro:include filename="$(find diffy)/models/urdf/robot.gazebo" />
<link name="base_link">
  <inertial>
    <origin xyz="0.0 0.0 0.0" rpy="0 0 0"/>
    <mass value="3.1399999999999997"/>
    <inertia ixx="0.002721" iyy="0.010571" izz="0.013083" ixy="0.0" iyz="0.0" ixz="0.0"/>
  </inertial>
  <visual>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <geometry>
      <mesh filename="file://$(find diffy)/models/meshes/base_link.stl" scale="0.001 0.001 0.001"/>
    </geometry>
    <material name="silver"/>
  </visual>
  <collision>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <geometry>
      <mesh filename="file://$(find diffy)/models/meshes/base_link.stl" scale="0.001 0.001 0.001"/>
    </geometry>
  </collision>
</link>

<link name="Wheel_1">
  <inertial>
    <origin xyz="0.035 -0.009999999999999981 5.981428822440112e-18" rpy="0 0 0"/>
    <mass value="0.3730052227515331"/>
    <inertia ixx="8.3e-05" iyy="0.000141" izz="8.3e-05" ixy="-0.0" iyz="-0.0" ixz="0.0"/>
  </inertial>
  <visual>
    <origin xyz="0.1 0.05 -0.0" rpy="0 0 0"/>
    <geometry>
      <mesh filename="file://$(find diffy)/models/meshes/Wheel_1.stl" scale="0.001 0.001 0.001"/>
    </geometry>
    <material name="silver"/>
  </visual>
  <collision>
    <origin xyz="0.1 0.05 -0.0" rpy="0 0 0"/>
    <geometry>
      <mesh filename="file://$(find diffy)/models/meshes/Wheel_1.stl" scale="0.001 0.001 0.001"/>
    </geometry>
  </collision>
</link>

<link name="Wheel_2">
  <inertial>
    <origin xyz="0.035 0.010000000000000016 -5.9814288224401115e-18" rpy="0 0 0"/>
    <mass value="0.3730052227515331"/>
    <inertia ixx="8.3e-05" iyy="0.000141" izz="8.3e-05" ixy="0.0" iyz="-0.0" ixz="-0.0"/>
  </inertial>
  <visual>
    <origin xyz="0.1 -0.05 -0.0" rpy="0 0 0"/>
    <geometry>
      <mesh filename="file://$(find diffy)/models/meshes/Wheel_2.stl" scale="0.001 0.001 0.001"/>
    </geometry>
    <material name="silver"/>
  </visual>
  <collision>
    <origin xyz="0.1 -0.05 -0.0" rpy="0 0 0"/>
    <geometry>
      <mesh filename="file://$(find diffy)/models/meshes/Wheel_2.stl" scale="0.001 0.001 0.001"/>
    </geometry>
  </collision>
</link>

<link name="CasterWheel_1">
  <inertial>
    <origin xyz="0.06999999999999999 -6.330200182349162e-18 -0.006562500000000001" rpy="0 0 0"/>
    <mass value="0.0881134927023249"/>
    <inertia ixx="7e-06" iyy="7e-06" izz="1.1e-05" ixy="-0.0" iyz="0.0" ixz="-0.0"/>
  </inertial>
  <visual>
    <origin xyz="-0.0 -0.0 0.01" rpy="0 0 0"/>
    <geometry>
      <mesh filename="file://$(find diffy)/models/meshes/CasterWheel_1.stl" scale="0.001 0.001 0.001"/>
    </geometry>
    <material name="silver"/>
  </visual>
  <collision>
    <origin xyz="-0.0 -0.0 0.01" rpy="0 0 0"/>
    <geometry>
      <mesh filename="file://$(find diffy)/models/meshes/CasterWheel_1.stl" scale="0.001 0.001 0.001"/>
    </geometry>
  </collision>
</link>

<joint name="Revolute 1" type="continuous">
  <origin xyz="-0.1 -0.05 0.0" rpy="0 0 0"/>
  <parent link="base_link"/>
  <child link="Wheel_1"/>
  <axis xyz="0.0 -1.0 0.0"/>
</joint>

<joint name="Revolute 2" type="continuous">
  <origin xyz="-0.1 0.05 0.0" rpy="0 0 0"/>
  <parent link="base_link"/>
  <child link="Wheel_2"/>
  <axis xyz="0.0 1.0 0.0"/>
</joint>

<joint name="Rigid 3" type="fixed">
  <origin xyz="0.0 0.0 -0.01" rpy="0 0 0"/>
  <parent link="base_link"/>
  <child link="CasterWheel_1"/>
</joint>

</robot>
